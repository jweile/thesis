	pmid = {16204249},
	pages = {40122--40129},
	file = {Full Text PDF:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/NARIR5A6/Song et al. - 2005 - Small Ubiquitin-like Modifier (SUMO) Recognition o.pdf:application/pdf;Snapshot:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/2T54J59Q/40122.html:text/html}
}

