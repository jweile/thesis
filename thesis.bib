	pages = {1515--1526},
	annote = {crispr screen},
	file = {ScienceDirect Full Text PDF:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/JXWX9E8W/Hart et al. - 2015 - High-Resolution CRISPR Screens Reveal Fitness Gene.pdf:application/pdf;ScienceDirect Snapshot:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/689KEBH7/S0092867415014956.html:text/html}
