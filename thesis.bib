}

@article{tasan_selecting_2015,
	title = {Selecting causal genes from genome-wide association studies via functionally coherent subnetworks},
	volume = {12},
	copyright = {© 2014 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1548-7091},
	url = {http://www.nature.com.myaccess.library.utoronto.ca/nmeth/journal/v12/n2/full/nmeth.3215.html#ref3},
	doi = {10.1038/nmeth.3215},
	abstract = {Genome-wide association (GWA) studies have linked thousands of loci to human diseases, but the causal genes and variants at these loci generally remain unknown. Although investigators typically focus on genes closest to the associated polymorphisms, the causal gene is often more distal. Reliance on published work to prioritize candidates is biased toward well-characterized genes. We describe a 'prix fixe' strategy and software that uses genome-scale shared-function networks to identify sets of mutually functionally related genes spanning multiple GWA loci. Using associations from {\textasciitilde}100 GWA studies covering ten cancer types, our approach outperformed the common alternative strategy in ranking known cancer genes. As more GWA loci are discovered, the strategy will have increased power to elucidate the causes of human disease.},
	language = {en},
