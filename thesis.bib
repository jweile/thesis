	keywords = {BRAFV600E, deep sequencing, drug resistance, saturation mutagenesis, vemurafenib},
	pages = {124--133},
	file = {Snapshot:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/FB46B89K/abstract.html:text/html}
}

