@article{alontaga_rwd_2015,
	title = {{RWD} {Domain} as an {E}2 ({Ubc}9)-{Interaction} {Module}},
	volume = {290},
	issn = {0021-9258, 1083-351X},
	url = {http://www.jbc.org/content/290/27/16550},
	doi = {10.1074/jbc.M115.644047},
	abstract = {An RWD domain is a well conserved domain found through bioinformatic analysis of the human proteome sequence; however, its function has been unknown. Ubiquitin-like modifications require the catalysis of three enzymes generally known as E1, E2, and E3. We solved the crystal structure of the E2 for the small ubiquitin-like modifiers (SUMO) in complex with an RWD domain and confirmed the structure using solution NMR analysis. The binding surface of RWD on Ubc9 is located near the N terminus of Ubc9 that is known to be involved in noncovalent binding of the proteins in the conjugation machinery, including a domain of E1, SUMO, and an E3 ligase. NMR data indicate that the RWD domain does not bind to SUMO and E1. The interaction between RWD and Ubc9 has a Kd of 32 ± 4 μm. Consistent with the structure and binding affinity and in contrast to a previous report, the RWD domain and RWDD3 have minimal effects on global SUMOylation. The structural and biochemical information presented here forms the basis for further investigation of the functions of RWD-containing proteins.},
	language = {en},
	number = {27},
	urldate = {2017-02-05},
	journal = {Journal of Biological Chemistry},
	author = {Alontaga, Aileen Y. and Ambaye, Nigus D. and Li, Yi-Jia and Vega, Ramir and Chen, Chih-Hong and Bzymek, Krzysztof P. and Williams, John C. and Hu, Weidong and Chen, Yuan},
	month = jul,
	year = {2015},
	pmid = {25918163},
	keywords = {E2, nuclear magnetic resonance (NMR), RWD, small ubiquitin-like modifier (SUMO), Sumoylation, ubiquitylation (ubiquitination), X-ray Crystallography},
	pages = {16550--16559},
	file = {Full Text PDF:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/HQ38IFDM/Alontaga et al. - 2015 - RWD Domain as an E2 (Ubc9)-Interaction Module.pdf:application/pdf;Snapshot:/home/jweile/.zotero/zotero/40mw2hl1.default/zotero/storage/ZCV6WA2X/16550.html:text/html}
